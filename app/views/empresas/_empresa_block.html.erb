<h2><%= empresa.nombre %> <span>(<%= empresa.nombre_legal %>)</span></h2>
<ul>
  <li class="telefono">
    <span class="etiqueta">Teléfono:</span>
    <%= empresa.tel %>
  </li>
  <li class="email">
    <span class="etiqueta">Email</span>
    <%= empresa.email %>
  </li>
  <li class="dirección">
    <span class="etiqueta">Dirección:</span>
    <%= empresa.dir %>
  </li>
  <li class="ciudad">
    <span class="etiqueta">Ciudad:</span>
    <%= empresa.ciudad %>
  </li>
</ul>
